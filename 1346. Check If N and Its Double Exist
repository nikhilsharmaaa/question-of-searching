class Solution {
    public boolean checkIfExist(int[] arr) {
      
       int end = arr.length-1;
       for (int i =0; i<= end; i++){
           for (int j =i+1; j<=end;j++){
               if (arr[i]== arr[j]*2 || arr[i]*2== arr[j]){
                   return true ;
               }
               
           }
       }
       return false ;

    }
}
